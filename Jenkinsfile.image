/*
 * Sample from https://www.jenkins.io/doc/pipeline/tour/hello-world/
 * Runs a simple pipeline in a Python container image.
 * Requires the Docker Pipeline plugin.
 */

pipeline {
    agent { docker { image 'python:3.14.1-alpine3.23' } }
    stages {
        stage('build') {
            steps {
                sh 'python --version'
            }
        }
    }
}
